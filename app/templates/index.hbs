

<h1>index page </h1>
	
 {{#each posts as |post|}}
    <h4>
        <span>{{post.id}}.{{post.title}} </span>
    	<button type="button" {{action 'edit' post}} class="btn btn-primary">edit</button>
    	<button type="button" {{action 'delete' post}} class="btn btn-danger">delete</button>
    </h4> 
 	<div>{{post.body}}</div>
  {{/each}}

{{outlet}}

